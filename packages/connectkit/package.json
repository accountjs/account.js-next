{
  "name": "@accountjs/connect",
  "version": "0.0.1",
  "description": "Account.js Connect Kit",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "node build.js",
    "build:watch": "node build.js --watch",
    "dev": "pnpm build:watch & pnpm typegen:watch & pnpm css:watch",
    "prepare": "pnpm build",
    "prebuild": "pnpm typegen",
    "typecheck": "pnpm tsc --noEmit",
    "css": "tailwindcss -i ./src/index.css -o ./dist/index.css",
    "css:watch": "pnpm css --watch",
    "typegen": "tsc --emitDeclarationOnly || true",
    "typegen:watch": "tsc --emitDeclarationOnly --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "ethers": ">=5.5.1 < 6",
    "react": ">=17",
    "react-dom": ">=17",
    "wagmi": "0.12.6"
  },
  "dependencies": {
    "@accountjs/sdk": "workspace:*",
    "@accountjs/service-client": "workspace:*",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "ethers": "^5.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "0.12.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.17.12",
    "esbuild-style-plugin": "^1.6.1",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.2.1",
    "tailwindcss": "^3.2.7",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "wagmi": "0.12.6"
  }
}
